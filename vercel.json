{
    "version": 2,
    "builds": [
      {
        "src": "./server/index.js",  
        "use": "@vercel/node"
      },
      {
        "src": "./client/build",   
        "use": "@vercel/static"
      }
    ],
  
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/server/index.js"
      }
    ]
  }